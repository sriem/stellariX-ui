{
    "name": "@stellarix-ui/vue",
    "version": "0.0.1",
    "description": "Vue 3.5+ adapter for StellarIX UI",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup --minify",
        "dev": "tsup --watch",
        "lint": "eslint \"src/**/*.ts*\"",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
    },
    "dependencies": {
        "@stellarix-ui/core": "workspace:*",
        "@stellarix-ui/utils": "workspace:*"
    },
    "peerDependencies": {
        "vue": "^3.5.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@vue/test-utils": "^2.4.0",
        "eslint": "^9.0.0",
        "jsdom": "^22.1.0",
        "tsup": "^8.0.0",
        "typescript": "^5.7.0",
        "vitest": "^2.1.0",
        "vue": "^3.5.0"
    },
    "license": "MIT"
}