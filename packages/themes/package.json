{
    "name": "@stellarix-ui/themes",
    "version": "0.0.1",
    "description": "Beautiful theme system for StellarIX UI components",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist/**",
        "css/**"
    ],
    "scripts": {
        "build": "tsup --minify",
        "dev": "tsup --watch",
        "lint": "eslint src/**/*.ts",
        "test": "vitest run",
        "test:watch": "vitest",
        "generate:css": "tsx scripts/generate-css.ts"
    },
    "keywords": [
        "themes",
        "design-system",
        "css",
        "styling",
        "stellarix"
    ],
    "dependencies": {
        "@stellarix-ui/core": "workspace:*"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "tsx": "^4.20.3",
        "typescript": "^5.7.2",
        "vitest": "^2.1.5"
    },
    "license": "MIT"
}