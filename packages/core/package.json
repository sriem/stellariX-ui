{
    "name": "@stellarix/core",
    "version": "0.0.1",
    "description": "Core framework-agnostic implementation of StellarIX UI",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint \"src/**/*.ts*\"",
        "test": "vitest run",
        "test:watch": "vitest",
        "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
    },
    "devDependencies": {
        "eslint": "^9.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.7.0",
        "vitest": "^2.1.0"
    }
}