{
  "name": "@stellarix-ui/container",
  "version": "0.0.1",
  "description": "Framework-agnostic container component for StellarIX UI",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup --minify",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "ui",
    "component",
    "container",
    "headless",
    "framework-agnostic"
  ],
  "dependencies": {
    "@stellarix-ui/core": "workspace:*",
    "@stellarix-ui/utils": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.5"
  },
  "license": "MIT"
}
