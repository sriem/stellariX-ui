#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/dev/stellariX-ui/node_modules/.pnpm/vitest@2.1.0_@types+node@22.0.0_@vitest+ui@2.1.0_happy-dom@15.0.0/node_modules/vitest/node_modules:/mnt/d/dev/stellariX-ui/node_modules/.pnpm/vitest@2.1.0_@types+node@22.0.0_@vitest+ui@2.1.0_happy-dom@15.0.0/node_modules:/mnt/d/dev/stellariX-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/dev/stellariX-ui/node_modules/.pnpm/vitest@2.1.0_@types+node@22.0.0_@vitest+ui@2.1.0_happy-dom@15.0.0/node_modules/vitest/node_modules:/mnt/d/dev/stellariX-ui/node_modules/.pnpm/vitest@2.1.0_@types+node@22.0.0_@vitest+ui@2.1.0_happy-dom@15.0.0/node_modules:/mnt/d/dev/stellariX-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vitest@2.1.0_@types+node@22.0.0_@vitest+ui@2.1.0_happy-dom@15.0.0/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vitest@2.1.0_@types+node@22.0.0_@vitest+ui@2.1.0_happy-dom@15.0.0/node_modules/vitest/vitest.mjs" "$@"
fi
