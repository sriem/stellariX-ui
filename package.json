{
    "name": "@stellarix/ui",
    "version": "0.0.1",
    "description": "A next-generation framework-agnostic headless component library",
    "private": true,
    "homepage": "https://github.com/yourusername/stellariX-ui",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yourusername/stellariX-ui.git"
    },
    "bugs": {
        "url": "https://github.com/yourusername/stellariX-ui/issues"
    },
    "keywords": [
        "ui",
        "components",
        "headless",
        "framework-agnostic",
        "react",
        "vue",
        "svelte",
        "solid",
        "qwik",
        "angular",
        "web-components"
    ],
    "engines": {
        "node": ">=18",
        "pnpm": ">=8"
    },
    "packageManager": "pnpm@8.6.0",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@changesets/cli": "^2.26.2",
        "eslint": "^8.46.0",
        "prettier": "^3.0.0",
        "turbo": "^1.10.12",
        "typescript": "^5.1.6",
        "vitest": "^0.34.1",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.3"
    },
    "license": "MIT"
}