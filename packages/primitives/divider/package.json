{
    "name": "@stellarix/divider",
    "version": "0.0.1",
    "description": "Divider primitive for StellarIX UI - Visual separator between content",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "sideEffects": false,
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup --minify",
        "dev": "tsup --watch",
        "lint": "eslint src/**/*.ts",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "keywords": [
        "ui",
        "component",
        "divider",
        "headless",
        "framework-agnostic"
    ],
    "dependencies": {
        "@stellarix/core": "workspace:*",
        "@stellarix/utils": "workspace:*"
    },
    "devDependencies": {
        "@storybook/react-vite": "^8.6.14",
        "storybook": "^8.6.14",
        "tsup": "^8.0.0",
        "typescript": "^5.7.2",
        "vitest": "^2.1.5"
    },
    "license": "MIT"
}