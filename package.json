{
    "name": "@stellarix/ui",
    "version": "0.0.1",
    "description": "A next-generation framework-agnostic headless component library",
    "private": true,
    "homepage": "https://github.com/sriem/stellariX-ui",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sriem/stellariX-ui.git"
    },
    "bugs": {
        "url": "https://github.com/sriem/stellariX-ui/issues"
    },
    "keywords": [
        "ui",
        "components",
        "headless",
        "framework-agnostic",
        "react",
        "vue",
        "svelte",
        "solid",
        "qwik",
        "angular",
        "web-components"
    ],
    "engines": {
        "node": ">=18",
        "pnpm": ">=8"
    },
    "packageManager": "pnpm@8.6.0",
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "test:visual": "build-storybook && vitest run storyshots.test.js",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@changesets/cli": "^2.26.2",
        "@storybook/addon-a11y": "^8.6.14",
        "@storybook/addon-essentials": "^8.6.14",
        "@storybook/react": "^8.6.14",
        "@storybook/test": "^8.6.14",
        "@testing-library/jest-dom": "^6.1.0",
        "@testing-library/react": "^16.0.0",
        "@testing-library/user-event": "^14.5.0",
        "@types/jest-axe": "^3.5.9",
        "@types/node": "^22.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@vitejs/plugin-react": "^4.0.4",
        "@vitest/ui": "^2.1.0",
        "axe-core": "^4.7.3",
        "eslint": "^9.0.0",
        "happy-dom": "^15.0.0",
        "husky": "^8.0.3",
        "jest-axe": "^10.0.0",
        "lint-staged": "^13.2.3",
        "playwright": "^1.52.0",
        "prettier": "^3.3.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "svelte": "^5.0.0",
        "turbo": "^2.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.7.0",
        "vite": "^4.4.9",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^2.1.0",
        "vue": "^3.5.0"
    },
    "license": "MIT"
}